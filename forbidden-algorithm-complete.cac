    <!-- Navigation -->
    <div class="navigation">
        <div class="nav-dot active" data-layer="1" onclick="goToLayer(1)"></div>
        <div class="nav-dot" data-layer="2" onclick="goToLayer(2)"></div>
        <div class="nav-dot" data-layer="3" onclick="goToLayer(3)"></div>
        <div class="nav-dot" data-layer="4" onclick="goToLayer(4)"></div>
    </div>

    <div class="nav-arrow left" onclick="prevLayer()" id="prevArrow">‚Äπ</div>
    <div class="nav-arrow right" onclick="nextLayer()" id="nextArrow">‚Ä∫</div>

    <script>
        let currentLayer = 1;
        const totalLayers = 4;

        // Initialize typewriter effect
        document.addEventListener('DOMContentLoaded', function() {
            const typewriterElements = document.querySelectorAll('.typewriter');
            typewriterElements.forEach(element => {
                const text = element.getAttribute('data-text');
                element.innerHTML = '';
                typeWriter(element, text, 0);
            });

            // Initialize countdown timer
            startCountdown();
            
            // Initialize stock counter animation
            startStockCounter();
            
            // Add keyboard navigation
            document.addEventListener('keydown', function(e) {
                if (e.key === 'ArrowRight' || e.key === ' ') {
                    e.preventDefault();
                    nextLayer();
                } else if (e.key === 'ArrowLeft') {
                    e.preventDefault();
                    prevLayer();
                }
            });

            // Add touch/swipe support for mobile
            let startX = 0;
            let startY = 0;
            
            document.addEventListener('touchstart', function(e) {
                startX = e.touches[0].clientX;
                startY = e.touches[0].clientY;
            });
            
            document.addEventListener('touchend', function(e) {
                if (!startX || !startY) return;
                
                const endX = e.changedTouches[0].clientX;
                const endY = e.changedTouches[0].clientY;
                
                const diffX = startX - endX;
                const diffY = startY - endY;
                
                if (Math.abs(diffX) > Math.abs(diffY) && Math.abs(diffX) > 50) {
                    if (diffX > 0) {
                        nextLayer(); // Swipe left - go to next
                    } else {
                        prevLayer(); // Swipe right - go to previous
                    }
                }
                
                startX = 0;
                startY = 0;
            });
        });

        function typeWriter(element, text, i) {
            if (i < text.length) {
                element.innerHTML += text.charAt(i);
                setTimeout(() => typeWriter(element, text, i + 1), 50);
            }
        }

        function nextLayer() {
            if (currentLayer < totalLayers) {
                goToLayer(currentLayer + 1);
            }
        }

        function prevLayer() {
            if (currentLayer > 1) {
                goToLayer(currentLayer - 1);
            }
        }

        function goToLayer(layerNumber) {
            // Remove active class from current layer
            document.getElementById(`layer${currentLayer}`).classList.remove('active');
            
            // Remove active class from current nav dot
            document.querySelector(`.nav-dot[data-layer="${currentLayer}"]`).classList.remove('active');
            
            // Update current layer
            currentLayer = layerNumber;
            
            // Add active class to new layer
            document.getElementById(`layer${currentLayer}`).classList.add('active');
            
            // Add active class to new nav dot
            document.querySelector(`.nav-dot[data-layer="${currentLayer}"]`).classList.add('active');
            
            // Update navigation arrows visibility
            updateNavArrows();
            
            // Trigger layer-specific animations
            triggerLayerAnimations(currentLayer);
        }

        function updateNavArrows() {
            const prevArrow = document.getElementById('prevArrow');
            const nextArrow = document.getElementById('nextArrow');
            
            if (currentLayer === 1) {
                prevArrow.style.opacity = '0.3';
                prevArrow.style.pointerEvents = 'none';
            } else {
                prevArrow.style.opacity = '1';
                prevArrow.style.pointerEvents = 'all';
            }
            
            if (currentLayer === totalLayers) {
                nextArrow.style.opacity = '0.3';
                nextArrow.style.pointerEvents = 'none';
            } else {
                nextArrow.style.opacity = '1';
                nextArrow.style.pointerEvents = 'all';
            }
        }

        function triggerLayerAnimations(layer) {
            // Reset and trigger animations for specific layers
            if (layer === 2) {
                const features = document.querySelectorAll('.feature-item');
                features.forEach((feature, index) => {
                    feature.style.animation = 'none';
                    setTimeout(() => {
                        feature.style.animation = `slideInFeature 0.6s ease-out ${index * 0.2}s forwards`;
                    }, 50);
                });
            } else if (layer === 3) {
                const tipCards = document.querySelectorAll('.tip-card');
                tipCards.forEach((card, index) => {
                    card.style.animation = 'none';
                    setTimeout(() => {
                        card.style.animation = `tipCardReveal 0.8s ease-out ${index * 0.2}s forwards`;
                    }, 50);
                });
            }
        }

        function startCountdown() {
            let timeLeft = 2 * 3600 + 47 * 60 + 33; // 2:47:33 in seconds
            
            function updateTimer() {
                const hours = Math.floor(timeLeft / 3600);
                const minutes = Math.floor((timeLeft % 3600) / 60);
                const seconds = timeLeft % 60;
                
                const display = `${hours.toString().padStart(2, '0')}:${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
                const timerElement = document.getElementById('countdown');
                if (timerElement) {
                    timerElement.textContent = display;
                }
                
                if (timeLeft > 0) {
                    timeLeft--;
                    setTimeout(updateTimer, 1000);
                } else {
                    if (timerElement) {
                        timerElement.textContent = "EXPIRED!";
                        timerElement.style.color = "#ff4444";
                    }
                }
            }
            
            updateTimer();
        }

        function startStockCounter() {
            let stock = 13;
            const stockElement = document.getElementById('stock');
            
            // Simulate occasional stock decreases
            function updateStock() {
                if (Math.random() < 0.1 && stock > 1) { // 10% chance every interval
                    stock--;
                    if (stockElement) {
                        stockElement.textContent = stock;
                        
                        // Add flash effect
                        stockElement.style.color = "#ff4444";
                        setTimeout(() => {
                            if (stockElement) {
                                stockElement.style.color = "#ffffff";
                            }
                        }, 500);
                    }
                }
                
                // Check again in 30-60 seconds
                setTimeout(updateStock, (30 + Math.random() * 30) * 1000);
            }
            
            updateStock();
        }

        // Initialize navigation arrows visibility
        updateNavArrows();

        // Add particle creation
        function createParticle() {
            const particle = document.createElement('div');
            particle.className = 'particle';
            particle.style.cssText = `
                position: absolute;
                width: 2px;
                height: 2px;
                background: #00ffff;
                border-radius: 50%;
                pointer-events: none;
                left: ${Math.random() * 100}%;
                top: 100%;
                box-shadow: 0 0 10px #00ffff;
                z-index: 1;
            `;
            
            document.body.appendChild(particle);
            
            // Animate particle
            particle.animate([
                { transform: 'translateY(0px)', opacity: 0 },
                { transform: 'translateY(-20px)', opacity: 1, offset: 0.1 },
                { transform: 'translateY(-100vh)', opacity: 0 }
            ], {
                duration: 8000,
                easing: 'linear'
            }).onfinish = () => {
                if (particle.parentNode) {
                    particle.parentNode.removeChild(particle);
                }
            };
        }

        // Create particles periodically
        setInterval(createParticle, 2000);

        // Add matrix rain characters
        function createMatrixRain() {
            const chars = '01';
            const container = document.querySelector('.matrix-rain');
            if (!container) return;
            
            for (let i = 0; i < 5; i++) {
                const column = document.createElement('div');
                column.style.cssText = `
                    position: absolute;
                    top: -100vh;
                    left: ${Math.random() * 100}%;
                    font-family: 'Courier New', monospace;
                    font-size: 12px;
                    color: #00ff00;
                    opacity: 0.3;
                    pointer-events: none;
                `;
                
                let text = '';
                for (let j = 0; j < 50; j++) {
                    text += chars[Math.floor(Math.random() * chars.length)] + '<br>';
                }
                column.innerHTML = text;
                
                container.appendChild(column);
                
                column.animate([
                    { transform: 'translateY(0vh)' },
                    { transform: 'translateY(200vh)' }
                ], {
                    duration: 15000,
                    easing: 'linear'
                }).onfinish = () => {
                    if (column.parentNode) {
                        column.parentNode.removeChild(column);
                    }
                };
            }
        }

        // Create matrix rain periodically
        setInterval(createMatrixRain, 3000);
        createMatrixRain(); // Initial call

        // Performance optimization: throttle animations on lower-end devices
        const isLowEndDevice = navigator.hardwareConcurrency < 4 || navigator.deviceMemory < 4;
        if (isLowEndDevice) {
            document.documentElement.style.setProperty('--animation-duration', '1s');
        }
    </script>
